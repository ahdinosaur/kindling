{
  "name": "kindling",
  "version": "0.0.0",
  "description": "language agnostic project bootstraping with an emphasis on simplicity",
  "main": "index.js",
  "scripts": {
    "start": "node-dev example",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/kindling.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/kindling/issues"
  },
  "homepage": "https://github.com/ahdinosaur/kindling#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "array-range": "^1.0.1",
    "enquirer": "^0.4.1",
    "enquirer-prompts": "^0.1.1",
    "es2040": "^1.2.3",
    "pixie": "github:ahdinosaur/pixie",
    "pull-stream": "^3.5.0",
    "pull-vinyl": "^1.2.0",
    "random-word": "^2.0.0",
    "spdx-license-list": "^3.0.1",
    "username": "^2.3.0"
  }
}
